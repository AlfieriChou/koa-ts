{
  "name": "koa-ts",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "scripts": {
    "dev": "ts-node-dev src/index.ts",
    "start": "ts-node src/index.ts",
    "migration:create": "ts-node ./node_modules/.bin/typeorm migration:create -n Migration",
    "migration:run": "ts-node ./node_modules/.bin/typeorm migration:run",
    "migration:revert": "ts-node ./node_modules/.bin/typeorm migration:revert"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlfieriChou/koa-ts.git"
  },
  "keywords": [
    "koa",
    "typescript"
  ],
  "author": "AlfieriChou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlfieriChou/koa-ts/issues"
  },
  "homepage": "https://github.com/AlfieriChou/koa-ts#readme",
  "devDependencies": {
    "@types/node": "^10.12.9",
    "ts-node": "7.0.1",
    "ts-node-dev": "^1.0.0-pre.30",
    "typescript": "3.1.6"
  },
  "dependencies": {
    "koa": "^2.6.1",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "mysql": "^2.14.1",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.9"
  }
}
